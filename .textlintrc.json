{
  "plugins": {
    "@textlint/markdown": {
      "extensions": [".mdx"]
    }
  },
  "filters": {},
  "rules": {
    "preset-ja-technical-writing": {
      "sentence-length": false,
      "no-exclamation-question-mark": false
    },
    "preset-japanese": {
      "sentence-length": {
        "max": 200
      }
    }
  }
}
